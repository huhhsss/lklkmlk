
from math import pi

class szescian():
    def __init__(self, bokszescian):
        self.bokszescian = bokszescian


    def informacjeszescian(self):
        print(f"pole = {self.bokszescian * self.bokszescian}")


a = int(input())

b1 = szescian(a)

lista = [b1]

for obj in lista:
    obj.informacjeszescian()


class prostopadloscian():
    def __init__(self, bokprostopadloscian1, bokprostopadloscian2, wysokoscszescian):
        self.bokprostopadloscian1 = bokprostopadloscian1
        self.bokprostopadloscian2 = bokprostopadloscian2
        self.wysokoscszescian = wysokoscszescian


    def informacjeprostopadloscian(self):
        print(f"pole = {self.bokprostopadloscian1 * self.bokprostopadloscian2 * self.wysokoscszescian}")

b = int(input())
c = int(input())
d = int(input())

b2 = prostopadloscian(b, c, d)
    
lista2 = [b2]

for obj in lista2:
    obj.informacjeprostopadloscian()



class walec():
    def __init__(self, promienwalca, wysokoscwalca):
        self.promienwalca = promienwalca
        self.wysokoscwalca = wysokoscwalca


    def informacjewalec(self):
        print(f"pole = {pi * self.promienwalca^2 * self.wysokoscwalca}")

e = int(input())
f = int(input())

b3 = walec(e, f)

lista3 = [b3]

for obj in lista3:
    obj.informacjewalec()


class stozek():
    def __init__(self, promienstozka, wysokoscstozka):
        self.promienstozka = promienstozka
        self.wysokoscstozka = wysokoscstozka


    def informacjestozek(self):
        print(f"pole = {1/3 * pi * self.promienstozka^2 * self.wysokoscstozka}")

g = int(input())
h = int(input())

b4 = stozek(g, h)

lista4 = [b4]

for obj in lista4:
    obj.informacjestozek()


class kula():
    def __init__(self, promienkuli):
        self.promienkuli= promienkuli


    def informacjekula(self):
        print(f"pole = {4/3 * pi * self.promienkuli**3}")


i = int(input())

b5 = kula(i)

lista5 = [b5]

for obj in lista5:
    obj.informacjekula()

    

class ostroslop():
    def __init__(self, polepodstawyostroslupa, wysokoscostroslupa):
        self.polepodstawyostroslupa = polepodstawyostroslupa
        self.wysokoscostroslupa = wysokoscostroslupa


    def informacjeostroslup(self):
        print(f"pole = {1/3 * self.polepodstawyostroslupa * self.wysokoscostroslupa}")


j = int(input())
k = int(input())

b6 = ostroslop(j, k)

lista6 = [b6]


for obj in lista6:
    obj.informacjeostroslup()


class graniastoslup():
    def __init__(self, polepodstawygraniastoslupa, wysokoscgraniastoslupa):
        self.polepodstawygraniastoslupa = polepodstawygraniastoslupa
        self.wysokoscgraniastoslupa = wysokoscgraniastoslupa


    def informacjegraniastoslup(self):
        print(f"pole = {self.polepodstawygraniastoslupa * self.wysokoscgraniastoslupa}")


l = int(input())
m = int(input())

b7 = graniastoslup(l, m)

lista7 = [b7]


for obj in lista7:
    obj.informacjegraniastoslup()



class kula2():
    def __init__(self, promienkuli2):
        self.promienkuli2 = promienkuli2


    def informacjekula2(self):
        print(f"pole = {4/3 * pi * self.promienkuli2**3 * 1/2}")


o = int(input())

b8 = kula2(o)

lista8 = [b8]

for obj in lista8:
    obj.informacjekula2()



class stozek2():
    def __init__(self, promienstozka2, wysokoscstozka2):
        self.promienstozka2 = promienstozka2
        self.wysokoscstozka2 = wysokoscstozka2


    def informacjestozek2(self):
        print(f"pole = {1/3 * pi * self.promienstozka2**2 * self.wysokoscstozka2*1/2}")

p = int(input())
r = int(input())

b9 = stozek2(p, r)

lista9 = [b9]

for obj in lista9:
    obj.informacjestozek2()


class donut():
    def init(self, promiendonuta, promiensrodkadonuta):
        self.pomiendonuta = promiendonuta
        self.promiensrodkadonuta = promiensrodkadonuta

    def informacjedonut(self):
        print(f"objetosc donuta = {pi**2*self.pomiendonuta*self.promiensrodkadonuta**2}")

s = int(input())
t = int(input())

b10 = donut(s, t)

lista10 = [b10]

for obj in lista10:
    obj.informacjedonut()



class donut2():
    def init(self, promiendonuta2, promiensrodkadonuta2):
        self.pomiendonuta2 = promiendonuta2
        self.promiensrodkadonuta2 = promiensrodkadonuta2

    def informacjedonut2(self):
        print(f"objetosc donuta = {pi**2*self.pomiendonuta2*self.promiensrodkadonuta2**2*1/2}")

u = int(input())
w = int(input())

b11 = donut2(u, w)

lista11 = [b11]

for obj in lista11:
    obj.informacjedonut2()
